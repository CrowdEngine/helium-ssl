{
  "name": "CrowdEngine SSL Proxy",
  "stack": "heroku-18",
  "env": {
    "NODE_PORT": "/tmp/node.socket",
    "RESTY_AUTO_SSL_DIR": "/app/resty-auto-ssl"
  },
  "formation": {
    "web": {
      "quantity": 1,
      "size": "Standard-1X"
    },
    "node": {
      "quantity": 1,
      "size": "Standard-1X"
    }
  },
  "addons": [
    "heroku-redis"
  ],
  "buildpacks": [
    {
      "url": "https://github.com/blarralde/heroku-buildpack-openresty-auto-ssl"
    },
    {
      "url": "heroku/nodejs"
    }
  ]
}